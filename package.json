{
  "name": "@numberapi/microservice-vk-notification",
  "version": "0.1.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "rimraf dist && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "docker:prod": "docker compose --file docker-compose.production.yml up --build -d",
    "docker:stage": "docker compose --file docker-compose.stage.yml up --build -d",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org quick6response --project called_id_notification dist && sentry-cli sourcemaps upload --org quick6response --project called_id_notification dist"
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^5.7.0",
    "@nestjs/common": "^11.0.12",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.12",
    "@sentry/cli": "^2.43.0",
    "@sentry/node": "^9.9.0",
    "@sentry/types": "^9.9.0",
    "@vk-io/hear": "^1.1.1",
    "@vk-io/scenes": "^1.2.1",
    "@vk-io/session": "^2.2.1",
    "middleware-io": "^2.8.1",
    "nestjs-pino": "^4.4.0",
    "nestjs-vk": "github:quick6response/nestjs-vk#73872def",
    "numberapi": "github:quick6response/number_api_event#607c8a6e",
    "pino-pretty": "^13.0.0",
    "pino-sentry": "^0.15.0",
    "redis": "^4.7.0",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.2",
    "telejson": "^7.2.0",
    "testing": "^3.1.0",
    "typescript": "^5.4.5",
    "vk-io": "^4.9.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.1",
    "@eslint/eslintrc": "^2.1.4",
    "@eslint/js": "^8.57.1",
    "@nestjs/cli": "^11.0.5",
    "@nestjs/schematics": "^11.0.2",
    "@nestjs/testing": "^11.0.12",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.13",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^16.0.0",
    "jest": "29.7.0",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "ts-jest": "29.3.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript-eslint": "^8.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
