{
  "name": "number-api-vk",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "rimraf dist && nest build && pnpm sentry:sourcemaps",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "SET NODE_ENV=development & nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "SET NODE_ENV=production & node dist/main",
    "docker": "docker compose  up --build",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org quick6response --project called_id_notification dist && sentry-cli sourcemaps upload --org quick6response --project called_id_notification dist"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.8",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/microservices": "^10.3.8",
    "@nestjs/platform-express": "^10.3.8",
    "@quick_response/number_api_event": "1.5.2-4",
    "@sentry/cli": "^2.31.2",
    "@sentry/node": "^8.0.0",
    "@sentry/tracing": "^7.114.0",
    "@vk-io/hear": "^1.1.1",
    "@vk-io/scenes": "^1.2.1",
    "@vk-io/session": "^2.2.1",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.4",
    "cache-manager": "^5.5.2",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "nestjs-vk": "^4.6.0",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "vk-io": "^4.9.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.8",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.10",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "29.7.0",
    "ngrok": "^4.3.3",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}